{
  "name": "item-butcher",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "author": "Alex Blount",
  "license": "MIT",
  "homepage": "https://alexlblount.github.io/item-butcher/",
  "scripts": {
    "build": "tsc && vite build",
    "build:now": "vite build",
    "build-and-deploy": "yarn build:now && yarn deploy",
    "deploy": "gh-pages -d dist --dest item-butcher",
    "dev": "vite",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx,.css,.scss --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "prettier --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,scss,md,json}\" ",
    "lint": "yarn lint:format && yarn lint:fix ",
    "preview": "vite preview",
    "transform-affixes": "node scripts/transformAffixes.cjs",
    "transform-aspects": "node scripts/transformAspects.cjs",
    "transform-data": "yarn transform-affixes && yarn transform-aspects",
    "zzlint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.1",
    "classnames": "^2.5.1",
    "i18next": "^23.7.16",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "modern-normalize": "^2.0.0",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.0",
    "react-json-view": "^1.21.3",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.21.1",
    "sort-by": "^1.2.0",
    "tesseract.js": "^5.0.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "gh-pages": "^6.1.1",
    "prettier": "^3.1.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-tsconfig-paths": "^4.2.3",
    "vitest": "^1.1.3"
  }
}
